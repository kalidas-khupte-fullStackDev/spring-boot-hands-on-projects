<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->

    <bean id="specification" class="com.example.autowire.byName.Specification">
        <property name="modelName" value="XUV-500"/>
        <property name="companyMakerName" value="Mahindra"/>
        <property name="manufactureYear" value="2015"/>
    </bean>

    <bean id="specification1" class="com.example.autowire.byName.Specification">
        <property name="modelName" value="XUV-600"/>
        <property name="companyMakerName" value="Mahindra"/>
        <property name="manufactureYear" value="2025"/>
    </bean>

    <bean id="carRef" class="com.example.autowire.byName.Car" autowire="byName">
    </bean>
</beans>